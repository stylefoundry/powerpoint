<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Relationships xmlns="http://schemas.openxmlformats.org/package/2006/relationships">
<% masters.each_with_index do |master,index| %><Relationship Id="rId<%= master[:id] %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/slideMaster" Target="<%= master[:file_path].gsub('../','') %>"/><% end %>
<% notes_masters.each_with_index do |master,index| %><Relationship Id="rId<%= master[:id] %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/notesMaster" Target="<%= master[:file_path].gsub('../','') %>"/><% end %>
<% slides.each_with_index do |slide, index| %><% id = rel_index + (index+1) %><Relationship Id="rId<%= id %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/slide" Target="slides/slide<%= index+1 %>.xml"/><% end %>
  <Relationship Id="rId<%= rel_index + slides.length + 1 %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/presProps" Target="presProps.xml"/>
  <Relationship Id="rId<%= rel_index + slides.length + 2 %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/viewProps" Target="viewProps.xml"/>
  <Relationship Id="rId<%= rel_index + slides.length + 3 %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/theme" Target="<%= themes.first[:file_path].gsub('../','') %>"/>
  <Relationship Id="rId<%= rel_index + slides.length + 4 %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/tableStyles" Target="tableStyles.xml"/>
  <Relationship Id="rId<%= rel_index + slides.length + 5 %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/customXmll+xml" Target="../customXml/item1.xml"/>
  <Relationship Id="rId<%= rel_index + slides.length + 6 %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/customXml+xml" Target="../customXml/item2.xml"/>
  <Relationship Id="rId<%= rel_index + slides.length + 7 %>" Type="http://schemas.openxmlformats.org/officeDocument/2006/relationships/customXml+xml" Target="../customXml/item3.xml"/>
</Relationships>
